let fst = true;;
let snd = false;;

let tuple a b = fun x -> if x == fst then a else b;;

let is_nil = false;;
let is_cons = true;;

let nil = tuple is_nil ();;
let cons a b = tuple is_cons (tuple a b);;

let lst = cons 1 (cons 2 (cons 3 nil));;

let find n lst =
  let rec aux i l =
    if l fst == is_nil then
      -1
    else if l snd fst == n then
      i
    else aux (i+1) (l snd snd)
  in
    aux 0 lst;;

