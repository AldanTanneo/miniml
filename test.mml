let rec phi x =
  if x == 0 then
    0
  else if x == 1 then
    1
  else
    phi (x - 1) + phi (x - 2)
;;

let phi2 x =
  let rec aux x a b =
    if x == 0 then
      a
    else
      aux (x - 1) b (a + b)
  in
    aux x 0 1
